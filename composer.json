{
    "name": "neutronstars/symfony-normalizer-enum-php",
    "description": "Added Symfony normalizer for the enumeration system designed to be as close to PHP 8.1 for earlier versions.",
    "type": "library",
    "license": [ "Apache-2.0" ],
    "authors": [
        {
            "name": "NeutronStars",
            "email": "pro@neutronstars.fr"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.1 | ^8.0",
        "neutronstars/enum": "^1.4.0",
        "symfony/serializer": "^4.0 | ^5.0"
    },
    "autoload": {
        "psr-4": {
            "NeutronStars\\Symfony\\Enum\\Normalizer\\": "src/"
        },
        "exclude-from-classmap": [
            "/tests/"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0 | ^7.0 | ^8.0 | ^9.0"
    },
    "autoload-dev": {
        "psr-4": {
            "NeutronStars\\Symfony\\Enum\\Normalizer\\Tests\\": "tests/"
        }
    }
}
